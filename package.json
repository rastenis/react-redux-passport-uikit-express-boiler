{
  "name": "react-redux-passport-uikit-express-boiler",
  "version": "1.0.2",
  "description": "A React+Redux boilerplate using Express as backend, UIKit for frontend, MongoDB for storage & Passport for auth.",
  "main": "index.js",
  "scripts": {
    "deploy": "echo \"Error: no deployment options yet\" && exit 1",
    "build": "webpack -p",
    "dev": "concurrently \"npm run dev-client\" \"npm run dev-server\"",
    "dev-server": "nodemon src/server --exec babel-node src/server",
    "dev-client": "webpack-dev-server --host 0.0.0.0",
    "start": "cross-env NODE_ENV=production babel-node src/server",
    "launch": "npm run build; npm run start",
    "setup": "node scripts/setup.js",
    "setup:default": "node scripts/setup.default.js",
    "config": "npm run setup",
    "test": "jest --verbose",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "author": "Scharkee",
  "repository": {
    "type": "git",
    "url": "https://repo.hepha.co.uk/root/react-redux-passport-uikit-express-boiler"
  },
  "license": "ISC",
  "dependencies": {
    "await-to-js": "^2.1.1",
    "axios": "^0.19.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "connect-mongo": "^3.0.0",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "express-session": "^1.16.2",
    "faker": "^4.1.0",
    "fs-extra": "^8.1.0",
    "greenlock-express": "^2.7.11",
    "greenlock-store-fs": "^3.0.2",
    "helmet": "^3.19.0",
    "mongoose": "^5.6.5",
    "passport": "^0.4.0",
    "passport-google-oauth": "^2.0.0",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "react": "^16.8.6",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.0.5",
    "shortid": "^2.2.14",
    "uikit": "^3.1.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.5.5",
    "axios-mock-adapter": "^1.17.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "concurrently": "^4.1.1",
    "cross-env": "^5.2.0",
    "css-loader": "^3.1.0",
    "jest": "^24.8.0",
    "jest-cli": "^24.8.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "nodemon": "^1.19.1",
    "prompt-sync": "^4.1.7",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga-test-plan": "^4.0.0-beta.3",
    "style-loader": "^1.0.0",
    "svg-inline-loader": "^0.8.0",
    "webpack": "^4.36.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/test/__mocks__/style.js"
    }
  }
}
